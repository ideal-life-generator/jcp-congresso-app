<?xml version='1.0' encoding='utf-8'?>
<widget id="no.jcp.congresso" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <preference name="AndroidLaunchMode" value="singleTop" />
    <feature name="App">
        <param name="android-package" value="org.apache.cordova.App" />
    </feature>
    <feature name="BarcodeScanner">
        <param name="android-package" value="com.phonegap.plugins.barcodescanner.BarcodeScanner" />
    </feature>
    <name>Atea Community 2014</name>
    <description>
        The majestic congresso app.
    </description>
    <author href="http://jcp.no">
        JCP
    </author>
    <content src="index.html" />
    <feature name="http://api.phonegap.com/1.0/device" />
    <feature name="http://api.phonegap.com/1.0/camera" />
    <preference name="Fullscreen" value="true" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="orientation" value="portrait" />
    <preference name="target-device" value="universal" />
    <preference name="webviewbounce" value="true" />
    <preference name="stay-in-webview" value="false" />
    <preference name="ios-statusbarstyle" value="black-opaque" />
    <preference name="exit-on-suspend" value="false" />
    <preference name="show-splash-screen-spinner" value="true" />
    <preference name="auto-hide-splash-screen" value="true" />
    <preference name="disable-cursor" value="false" />
    <preference name="android-minSdkVersion" value="15" />
    <preference name="android-installLocation" value="auto" />
    <icon src="www/img/icons/icon-144.png" />
    <icon gap:density="ldpi" gap:platform="android" src="www/img/icons/icon-144.png" />
    <icon gap:density="mdpi" gap:platform="android" src="www/img/icons/icon-144.png" />
    <icon gap:density="hdpi" gap:platform="android" src="www/img/icons/icon-144.png" />
    <icon gap:density="xhdpi" gap:platform="android" src="www/img/icons/icon-144.png" />
    <icon gap:platform="blackberry" src="www/img/icons/icon-144.png" />
    <icon gap:platform="blackberry" gap:state="hover" src="www/img/icons/icon-144.png" />
    <icon gap:platform="ios" height="57" src="www/img/icons/icon-57.png" width="57" />
    <icon gap:platform="ios" height="72" src="www/img/icons/icon-72.png" width="72" />
    <icon gap:platform="ios" height="114" src="www/img/icons/icon-114.png" width="114" />
    <icon gap:platform="ios" height="144" src="www/img/icons/icon-144.png" width="144" />
    <icon gap:platform="webos" src="www/img/icons/icon-144.png" />
    <icon gap:platform="winphone" src="www/img/icons/icon-144.png" />
    <icon gap:platform="winphone" gap:role="background" src="www/img/icons/icon-144.png" />
    <gap:splash gap:density="ldpi" gap:platform="android" src="www/img/Launch-Images/atea-bg.png" />
    <gap:splash gap:density="mdpi" gap:platform="android" src="www/img/Launch-Images/atea-bg.png" />
    <gap:splash gap:density="hdpi" gap:platform="android" src="www/img/Launch-Images/atea-bg.png" />
    <gap:splash gap:density="xhdpi" gap:platform="android" src="www/img/Launch-Images/atea-bg.png" />
    <gap:splash gap:platform="blackberry" src="www/img/Launch-Images/atea-bg.png" />
    <gap:splash gap:platform="ios" height="480" src="www/img/Launch-Images/Default~iphone.png" width="320" />
    <gap:splash gap:platform="ios" height="960" src="www/img/Launch-Images/Default@2x~iphone.png" width="640" />
    <gap:splash gap:platform="ios" height="1024" src="www/img/Launch-Images/Default-Portrait~ipad.png" width="768" />
    <gap:splash gap:platform="ios" height="768" src="www/img/Launch-Images/Default-Landscape~ipad.png" width="1024" />
    <gap:splash gap:platform="winphone" src="www/img/Launch-Images/SplashScreenImage.jpg" />
    <gap:plugin name="nl.x-services.plugins.socialsharing" />
    <plugin name="InAppBrowser" value="org.apache.cordova.InAppBrowser" />
    <access browserOnly="true" origin="*" />
    <feature name="InAppBrowser">
        <param name="android-package" value="org.apache.cordova.inappbrowser.InAppBrowser" />
    </feature>
    <feature name="UniversalAnalytics">
        <param name="ios-package" value="UniversalAnalyticsPlugin" />
        <param name="android-package" value="com.danielcwilson.plugins.analytics.UniversalAnalyticsPlugin" />
    </feature>
</widget>
